{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PublicReadAccess",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::eg-pz-prod-s3-branding-ohio/*"
    },
    {
      "Sid": "AllowAuthenticatedUploads",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::165183897698:user/cicd-devops"
      },
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": "arn:aws:s3:::eg-pz-prod-s3-branding-ohio/*"
    }
  ]
}